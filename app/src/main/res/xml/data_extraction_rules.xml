<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <include domain="root" />
        <include domain="file" path="." />
        <exclude domain="root" path="device_preferences" />
    </cloud-backup>
    <device-transfer>
        <include domain="root" />
        <include domain="file" path="." />
        <exclude domain="root" path="device_preferences" />
    </device-transfer>
</data-extraction-rules>